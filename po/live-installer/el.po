#: src/clients/inst_live_doit.rb:61 src/clients/inst_live_doit.rb:62
msgid "Copying root filesystem..."
msgstr "Αντιγραφή βασικού Σ/Α..."

#: src/clients/inst_live_doit.rb:63
msgid "Copying live image..."
