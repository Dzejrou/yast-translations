#: src/include/auth-server/kerberos_advanced.rb:53
#: src/include/auth-server/kerberos_advanced.rb:1885
msgid "Database Path"
msgstr "データベースのパス"

#: src/include/auth-server/kerberos_advanced.rb:60
msgid "&Database Path"
msgstr "データベースのパス (&D)"

#. Treeview list item
#: src/include/auth-server/kerberos_advanced.rb:71
