msgstr "myport"

#: src/modules/SquidACL.rb:232
msgid "Port Number"
msgstr "Номер порту"

#: src/modules/SquidACL.rb:237
msgid "Provides match for local TCP port number."
msgstr "Надає відповідність для номеру локального TCP-порту."

#: src/modules/SquidACL.rb:242
