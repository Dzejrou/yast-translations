msgid "Language and Keyboard Layout"
msgstr "Sprog og tastaturlayout"

#. combo box label
#: src/clients/firstboot_language_keyboard.rb:78
msgid "&Language"
msgstr "&Sprog"

#. combo box label
#: src/clients/firstboot_language_keyboard.rb:86
msgid "&Keyboard Layout"
msgstr "&Tastaturlayout"

#. this type of contents will be shown only for initial installation dialog
#: src/clients/firstboot_language_keyboard.rb:110
msgid "&Test"
msgstr ""

#. help text for firstboot language + keyboard screen
#: src/clients/firstboot_language_keyboard.rb:119
msgid ""
"<p>\n"
"Choose the <b>Language</b> and the <b>Keyboard Layout</b> to be used during\n"
"configuration and in the installed system.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Vælg <b>sprog</b> og <b>tastaturlayout</b> der skal bruges under\n"
"konfiguration og til det installerede system.\n"
"</p>\n"

#. help text, continued
#: src/clients/firstboot_language_keyboard.rb:126
msgid ""
"<p>\n"
"Click <b>Next</b> to proceed to the next dialog.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Tryk på <b>Næste</b> for at fortsætte til næste dialog.\n"
"</p>\n"

#. help text, continued
#: src/clients/firstboot_language_keyboard.rb:132
msgid ""
"<p>\n"
"Select <b>Abort</b> to abort the\n"
"installation process at any time.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Vælg <b>Afbryd</b> når som helst, \n"
"for at afbryde installationsprocessen.\n"
"</p>\n"

#. error message
#: src/clients/firstboot_language_keyboard.rb:273
msgid "There is not enough space to install all additional packages."
msgstr "Der er ikke nok plads til at installere alle yderligere pakker."

