#. Build dialog
#. ----------------------------------------------------------------------
#. TRANSLATORS: heading text
#: src/clients/firstboot_language_keyboard.rb:72
msgid "Language and Keyboard Layout"
msgstr "Idioma y distribución del teclado"

#
#. TRANSLATORS: combo box label
#: src/clients/firstboot_language_keyboard.rb:78
msgid "&Language"
msgstr "&Idioma"

# clients/inst_environment.ycp:232
#. TRANSLATORS: combo box label
#: src/clients/firstboot_language_keyboard.rb:86
msgid "&Keyboard Layout"
msgstr "Distribución del &teclado"

#. TRANSLATORS: input field label
#: src/clients/firstboot_language_keyboard.rb:111
msgid "&Test"
msgstr "&Prueba"

# clients/inst_language.ycp:133
#. TRANSLATORS: help text for firstboot language + keyboard screen
#: src/clients/firstboot_language_keyboard.rb:120
msgid ""
"<p>\n"
"Choose the <b>Language</b> and the <b>Keyboard Layout</b> to be used during\n"
"configuration and in the installed system.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Elija el <b>idioma</b> y la <b>distribución del teclado</b> que se usarán "
"durante la configuración y en\n"
"el sistema instalado.\n"
"</p>\n"

#. TRANSLATORS: help text, continued
#: src/clients/firstboot_language_keyboard.rb:127
msgid ""
"<p>\n"
"Click <b>Next</b> to proceed to the next dialog.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Haga clic en <b>Siguiente</b> para acceder al siguiente cuadro de diálogo.\n"
"</p>\n"

# clients/inst_language.ycp:156
#. TRANSLATORS: help text, continued
#: src/clients/firstboot_language_keyboard.rb:133
msgid ""
"<p>\n"
"Select <b>Abort</b> to abort the\n"
"installation process at any time.\n"
"</p>\n"
msgstr ""
"<p>\n"
"Seleccione <b>Cancelar</b> en cualquier momento\n"
"para cancelar el proceso de instalación.\n"
"</p>\n"

#. TRANSLATORS: error message
#: src/clients/firstboot_language_keyboard.rb:274
msgid "There is not enough space to install all additional packages."
msgstr ""
"No hay espacio suficiente para instalar todos los paquetes adicionales."

