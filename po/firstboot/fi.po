msgstr "&Ylemmäs"

#. TRANSLATORS: button label
#: src/clients/firstboot_config.rb:70
msgid "D&own"
msgstr "&Alemmas"

#. TRANSLATORS: button label
#: src/clients/firstboot_config.rb:72
msgid "Enab&le or Disable"
msgstr "&Ota käyttöön tai poista käytöstä"

